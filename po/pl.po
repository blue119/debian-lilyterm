#: console.c:140
#, c-format
msgid ""
"%s is a libvte based X Terminal Emulator.\n"
"\n"
msgstr ""
"%s jest emulatorem terminala opartym o libvte.\n"
"\n"

#: console.c:141
msgid "Use -T/--title {title} to specify the window title.\n"
msgstr "Użyj -T/--title {tytuł} aby ustawić tytuł okna.\n"

#: console.c:142
msgid "Use -t/--tab {number} to open multi tabs when starting up.\n"
msgstr "Użyj -t/--tab {liczba} aby otworzyć wiele kart przy starcie.\n"

#: console.c:143
msgid ""
"Use -n/--tab_names {tab names} to specify tab names, separate with <space>.\n"
msgstr ""

#: console.c:144
msgid ""
"Use -d/--directory {directory} to specify the init directory when starting "
"up.\n"
msgstr ""
"Użyj -d/--directory {katalog} aby ustawić katalog roboczy przy starcie.\n"

#: console.c:145
#, fuzzy
#| msgid ""
#| "Use -g/--geometry {WIDTHxHEIGHT[+-]XOFFSET[+-]YOFFSET} to specify the "
#| "geometry of window.\n"
msgid ""
"Use -g/--geometry {WIDTHxHEIGHT[+-]XOFFSET[+-]YOFFSET} to specify the "
"geometry.\n"
msgstr ""
"Użyj -g/--geometry {SZEROKOŚĆXWYSOKOŚĆ[+-]XOFFSET[+-]YOFFSET} aby ustawić "
"geometrię okna.\n"

#: console.c:146
msgid "Use -l/-ls/--login to make the shell invoked as a login shell.\n"
msgstr "Użyj -l/-ls/--login aby wywołać powłokę jako login shell.\n"

#: console.c:147
msgid ""
"Use -H/--hold to hold the terminal window open when -e/-x command "
"terminated.\n"
msgstr ""

#: console.c:149
msgid "Use -s/--separate to run in separate process.\n"
msgstr "Użyj -s/--separate aby uruchomić osobny proces.\n"

#: console.c:150
msgid ""
"Use -j/--join to integrate new created tabs to the last accessed window.\n"
msgstr ""

#: console.c:151
#, fuzzy
#| msgid "Use -p/--profile to got a profile sample.\n"
msgid "Use -p/--profile to get a profile sample.\n"
msgstr "Użyj -p/--profile aby zobaczyć przykładową konfigurację profilu.\n"

#: console.c:153
#, fuzzy, c-format
#| msgid ""
#| "Use -u/--user_profile {%s} to use a specified profile.\n"
